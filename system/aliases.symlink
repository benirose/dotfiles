# NPM
alias npmpatch='npm version patch && git push && npm publish'

# GIT
alias st='git status -s;for plugin in plugins/*/; do (cd $plugin; if [[ `git status -s .` ]]; then echo;echo $plugin; git status -s .;fi ;) done'
 
alias stree='open -a SourceTree ./'

alias git-sync-tags='git tag -l | xargs git tag -d && git fetch --tags'
alias git-trash='git reset --hard @{u}'
alias git-gone-prune="git fetch --prune | git branch -vv | grep ': gone]' | awk '{print \$1}' | xargs git branch -d"
alias git-gone="git fetch --prune | git branch -vv | grep ': gone]' | awk '{print \$1}'"
alias g='git'
alias ga='git add'
alias gp='git push'
alias gpa='gp --all'
alias gu='git pull'
alias gl='git log'
alias gg='gl --decorate --oneline --graph --date-order --all'
alias gs='git status'
alias gst='gs'
alias gd='git diff'
alias gdc='gd --cached'
alias gm='git commit -m'
alias gma='git commit -am'
alias gb='git branch'
alias gba='git branch -a'
function gc() { git checkout "${@:-master}"; } # Checkout master by default
alias gco='gc'
alias gcb='gc -b'
alias gr='git remote'
alias grv='gr -v'
#alias gra='git remote add'
alias grr='git remote rm'
alias gcl='git clone'
alias modulestatus="find . -maxdepth 1 -mindepth 1 -type d -exec sh -c '(echo {} && cd {} && git status -s && echo)' \;"

alias chrome="open -a 'Google Chrome'"

alias datafart='curl --data-binary @- datafart.com'

alias grepe='grep --exclude-dir=.svn --exclude-dir=log --exclude-dir=cache --exclude-dir=node_modules'

# WTH Dev specific

alias plugin='cd plugins/wayToHealthPlugin'

#sync prod
alias syncprod="./symfony w2h:sync-prod && d"

# get study url name
function name() { ll ~/www/waytohealth/web | grep $1; }

# open study for current folder
function dev() {
  name ${PWD##*/} | awk '{ system("open http://waytohealth/"$9) }'
}

# cat ummon log
alias ulog='ssh prod-tasks cat log/ummon/server.log'
# log an ummon run
function ulogrun() {
  ssh prod-tasks grep "$0" log/ummon/server.log | bunyan -L -c 'this.runid == "$0"'
}
# tail ummon log
function utail() {
  ssh prod-tasks tail -f log/ummon/server.log | gstdbuf -o0 bunyan -L -c 'this.taskid == "$1"'
}
